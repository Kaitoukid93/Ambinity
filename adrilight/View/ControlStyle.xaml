            <Setter.Value>
                <ControlTemplate TargetType="{x:Type CheckBox}">
                    <Border CornerRadius="24" BorderBrush="{TemplateBinding Foreground}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" Width="24" Height="24" >
                        <Viewbox Width="24" Height="24" Name="vb">
                            <Canvas Width="24" Height="24" Name="cv">
                                <Path Fill="{TemplateBinding Foreground}" Data="M16.56,5.44L15.11,6.89C16.84,7.94 18,9.83 18,12A6,6 0 0,1 12,18A6,6 0 0,1 6,12C6,9.83 7.16,7.94 8.88,6.88L7.44,5.44C5.36,6.88 4,9.28 4,12A8,8 0 0,0 12,20A8,8 0 0,0 20,12C20,9.28 18.64,6.88 16.56,5.44M13,3H11V13H13" />
                            </Canvas>
                        </Viewbox>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Cursor" Value="Hand"></Setter>
                        </Trigger>
                        <DataTrigger Binding="{Binding IsConnected}" Value="True">
                            <Setter Property="Background" Value="#e7e0f4"></Setter>
                            <Setter Property="Foreground" Value="#7344c3"></Setter>
                            <Setter Property="BorderThickness" Value="1"></Setter>
                            <Setter Property="Margin" Value="-1,0,0,0" TargetName="vb"></Setter>

                        </DataTrigger>

                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="btnCharacter" TargetType="{x:Type Button}">
        <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
        <Setter Property="OverridesDefaultStyle" Value="True"></Setter>
        <Setter Property="Background" Value="#d6d5d5"></Setter>
        <Setter Property="Foreground" Value="Gray"></Setter>
        <Setter Property="BorderThickness" Value="0"></Setter>
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="FontWeight" Value="Bold"></Setter>
        <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
        <Setter Property="HorizontalContentAlignment" Value="Center"></Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Image Name="imgCharacter" Source="{StaticResource svgGrayBalpha}" Width="{TemplateBinding Width}" Height="{TemplateBinding Height}"></Image>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Cursor" Value="Hand"></Setter>
                        </Trigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding IsConnected}" Value="True" />
                                <Condition Binding="{Binding StartChar}" Value="B"/>
                            </MultiDataTrigger.Conditions>
                            <Setter Property="Source" TargetName="imgCharacter" Value="{StaticResource svgPurpleB}"></Setter>
                        </MultiDataTrigger>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding IsConnected}" Value="False" />
                                <Condition Binding="{Binding StartChar}" Value="B"/>
                            </MultiDataTrigger.Conditions>
                            <Setter Property="Source" TargetName="imgCharacter" Value="{StaticResource svgGrayBalpha}"></Setter>
                        </MultiDataTrigger>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding IsConnected}" Value="True" />
                                <Condition Binding="{Binding StartChar}" Value="E"/>
                            </MultiDataTrigger.Conditions>
                            <Setter Property="Source" TargetName="imgCharacter" Value="{StaticResource svgPurpleE}"></Setter>
                        </MultiDataTrigger>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding IsConnected}" Value="False" />
                                <Condition Binding="{Binding StartChar}" Value="E"/>
                            </MultiDataTrigger.Conditions>
                            <Setter Property="Source" TargetName="imgCharacter" Value="{StaticResource svgGrayEAlpha}"></Setter>
                        </MultiDataTrigger>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding IsConnected}" Value="True" />
                                <Condition Binding="{Binding StartChar}" Value="H"/>
                            </MultiDataTrigger.Conditions>
                            <Setter Property="Source" TargetName="imgCharacter" Value="{StaticResource svgPurpleH}"></Setter>
                        </MultiDataTrigger>
                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding IsConnected}" Value="False" />
                                <Condition Binding="{Binding StartChar}" Value="H"/>
                            </MultiDataTrigger.Conditions>
                            <Setter Property="Source" TargetName="imgCharacter" Value="{StaticResource SvgGrayHAlpha}"></Setter>
                        </MultiDataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
