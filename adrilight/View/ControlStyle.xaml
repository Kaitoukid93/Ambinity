                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding IsConnected}" Value="True" />
                                <Condition Binding="{Binding StartChar}" Value="H"/>
                            </MultiDataTrigger.Conditions>
                            <Setter Property="Source" TargetName="imgCharacter" Value="{StaticResource svgPurpleH}"></Setter>
